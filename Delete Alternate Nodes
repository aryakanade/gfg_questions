'''class Node: 
   
    # Function to initialize the node object 
    def __init__(self, data): 
        self.data = data  
        self.next = None'''

class Solution:
    def deleteAlt(self, head):
        # code here
        curr = head
        while curr is not None and curr.next is not None:
            altnext = curr.next.next
            del curr.next
            
            curr.next = altnext
            curr = altnext
            
        return head
